@model IEnumerable<CANSnatcher.SubmissionView>

@{
    ViewBag.Title = "Submissions [Job Order - Candidate]";
    Layout = "~/Views/Working/_working_layout.cshtml";
}

<br /><br />
<p>
    <button class="btn-link" onclick="location.href='@Url.Action("Create", "Submissions", new {jo = 0, can = 0 }, null)'">New Submission</button>
</p>
<table class="table table-striped table-bordered">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.LNAME)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FNAME)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TITLE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Facility_NAME)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.JO_START_DATE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RATE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BILL_RATE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.JO_STATUS)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.JO_DESCRIPTION)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model)
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.LNAME)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FNAME)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TITLE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Facility_NAME)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.JO_START_DATE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RATE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BILL_RATE)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.JO_STATUS)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.JO_DESCRIPTION)
        </td>
        <td>
            <input type="button" class="btn-danger btn" value="Delete" onclick="location.href='@Url.Action("Delete", "SubmissionViews", new { id = item.UniqueID__Id } )'" />

        </td>
    </tr>
}

</table>
